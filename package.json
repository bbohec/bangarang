{
  "name": "bangarang",
  "version": "0.0.0",
  "description": "allows people to vote",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev:ui:old": "npm run build -- -w && parcel dist/ui/index.html",
    "dev:ui": "parcel src/ui/index.html",
    "build:ui": "parcel build src/ui/index.html",
    "build": "tsc",
    "build:css": "postcss src/ui/styles/src.css -o src/ui/styles/main.css",
    "test": "mocha src/tests/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/bbohec/bangarang.git"
  },
  "author": "Benjamin Bohec",
  "license": "CC-BY-NC-SA-4.0",
  "bugs": {
    "url": "https://gitlab.com/bbohec/bangarang/issues"
  },
  "homepage": "https://gitlab.com/bbohec/bangarang#readme",
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/express": "^4.17.9",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "chai": "^4.2.0",
    "eslint": "^7.9.0",
    "mocha": "^8.1.3",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "autoprefixer": "^10.0.2",
    "parcel": "^2.0.0-nightly.439",
    "postcss": "^8.1.9",
    "postcss-cli": "^8.3.0",
    "tailwindcss": "^2.0.1"
  },
  "mocha": {
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "full-trace": true,
    "reporter": "spec"
  },
  "dependencies": {
    "express": "^4.17.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
